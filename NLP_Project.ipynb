        "\n",
        "\n",
        "def get_lid_tags(tokens):\n",
        "    tags = []\n",
        "    for token in tokens:\n",
        "        if token.lower() in roman_urdu_words:\n",
        "            tags.append(1)  # Roman Urdu\n",
        "        else:\n",
        "            tags.append(0)  # English\n",
        "    return tags\n"
      ],
