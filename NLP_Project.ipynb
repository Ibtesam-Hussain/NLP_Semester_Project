    {
      "cell_type": "code",
      "source": [
        "from transformers import Trainer\n",
        "\n",
        "class LIDTrainer(Trainer):\n",
        "    # override the prediction_step to only use sentiment logits\n",
        "    def prediction_step(self, model, inputs, prediction_loss_only, ignore_keys=None):\n",
        "        inputs = self._prepare_inputs(inputs)\n",
        "        with torch.no_grad():\n",
        "            outputs = model(**inputs)\n",
        "            # use only sentiment logits for evaluation (ignore lid_logits)\n",
        "            logits = outputs[\"sentiment_logits\"]\n",
        "            loss = outputs.get(\"loss\", None)\n",
        "        labels = inputs.get(\"sentiment_labels\")\n",
        "        return (loss, logits, labels)\n"
      ],
      "metadata": {
        "id": "R0XNKOcDReNw"
      },
      "execution_count": 19,
